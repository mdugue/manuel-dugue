{
  "name": "manuel-dugue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "lint": "eslint components/ hooks/ pages/ --ext .jsx,.js,.tsx,.ts --fix"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^10.1.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vercel/node": "^1.9.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^4.1.0",
    "@next/bundle-analyzer": "^10.1.3",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@types/google-spreadsheet": "^3.0.2",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "chrome-aws-lambda": "^8.0.2",
    "google-spreadsheet": "^3.1.15",
    "next": "^10.1.3",
    "next-plugin-preact": "^3.0.4",
    "next-seo": "^4.23.0",
    "postcss": "^8.2.15",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "puppeteer": "^8.0.0",
    "react": "npm:@preact/compat@0.0.4",
    "react-dom": "npm:@preact/compat@0.0.4",
    "react-spring": "^8.0.27",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.1.3",
    "react-use": "^17.2.1",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.2.3",
    "typewriter-effect": "^2.17.0"
  },
  "eslintConfig": {
    "plugins": [
      "@next/next",
      "simple-import-sort"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "react-app",
      "plugin:@next/next/recommended"
    ],
    "rules": {
      "simple-import-sort/imports": "error",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "hrefLeft",
            "hrefRight"
          ],
          "aspects": [
            "invalidHref",
            "preferButton"
          ]
        }
      ],
      "@typescript-eslint/explicit-module-boundary-types": "off"
    },
    "overrides": [
      {
        "files": "next.config.js",
        "rules": {
          "simple-import-sort/imports": "off",
          "@typescript-eslint/no-var-requires": "off"
        }
      }
    ]
  }
}
